        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^5.2.0",
        "electron": "^9.0.5",
        "electron-builder": "^22.7.0",
        "electron-redux-devtools": "^0.3.4",
        "wait-on": "^5.0.1"
    },
    "proxy": "http://localhost:5000"
